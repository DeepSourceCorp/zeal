{
  "name": "@deepsourcelabs/zeal",
  "version": "0.3.90",
  "repository": "https://github.com/deepsourcelabs/bifrost",
  "main": "./dist/zeal.common.js",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name zeal src/main.ts",
    "build-lib-icons": "yarn build_icons && vue-cli-service build --target lib --name zeal src/main.ts && yarn build:css",
    "test:unit": "vue-cli-service test:unit",
    "inspect": "vue-cli-service inspect",
    "tailwindcss:build": "tailwindcss build src/assets/css/tailwind.css -o src/assets/css/index.css",
    "style": "tailwindcss build src/assets/styles/index.css -o tailwindcss/tailwind.css",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build -c .storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c .storybook",
    "build-storybook": "yarn storybook:build",
    "chromatic": "npx chromatic --project-token kbbvog2zr9",
    "build_icons": "node bin/build-icons.js",
    "build:css": "postcss src/assets/css/tailwind.css -o dist/tailwind.css && postcss src/assets/css/typography.css -o dist/typography.css && postcss src/assets/css/layout.css -o dist/layout.css && postcss src/assets/css/chart.css -o dist/chart.css"
  },
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@tailwindcss/typography": "^0.4.0",
    "core-js": "^3.11.2",
    "frappe-charts": "^2.0.0-rc17",
    "postcss": "^7",
    "postcss-cli": "7.1.2",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/vue": "^6.1.21",
    "@types/feather-icons": "^4.7.0",
    "@types/istanbul-lib-instrument": "^1.7.4",
    "@types/jest": "^24.0.19",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.3",
    "chromatic": "^5.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "feather-icons": "^4.28.0",
    "html-minifier": "^4.0.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss-import": "12.0.1",
    "postcss-loader": "4.0.1",
    "prettier": "^1.19.1",
    "sass-loader": "7.1.0",
    "typescript": "~3.9.3",
    "vue-class-component": "^7.2.6",
    "vue-cli-plugin-storybook": "~2.0.0",
    "vue-cli-plugin-tailwind": "~1.4.1",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": false
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ]
}
